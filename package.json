{
  "name": "wa-automate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node --icu-data-dir=node_modules/full-icu ./src/bin/www",
    "dev": "nodemon --icu-data-dir=node_modules/full-icu ./src/bin/www",
    "lint": "eslint --fix --ext .js,.jsx .",
    "populate": "node dummy",
    "postinstall": "husky install"
  },
  "_moduleAliases": {
    "@api": "src/api",
    "@models": "src/models",
    "@data": "data",
    "@utils": "src/utils",
    "@database": "src/database",
    "@constants": "src/constants",
    "@config": "src/config",
    "@controllers": "src/controllers",
    "@middlewares": "src/middlewares",
    "@routes": "src/routes"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "bcrypt": "^5.0.1",
    "consolidate": "^0.15.1",
    "convert-units": "^2.3.4",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongoose": "^5.12.12",
    "mongoose-paginate-v2": "^1.3.18",
    "mongoose-type-email": "^1.1.2",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.4",
    "sequelize": "^6.6.2",
    "swig": "^1.4.2"
  },
  "devDependencies": {
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.20.0",
    "husky": "^5.1.1",
    "movies-names": "^1.1.1",
    "node-random-name": "^1.0.1",
    "pick-random": "^3.0.0",
    "random": "^3.0.6",
    "random-birthday": "^1.0.1",
    "random-email": "^1.0.3",
    "randomstring": "^1.2.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
